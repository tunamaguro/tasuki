{
    "version": "2",
    "plugins": [
        {
            "name": "sqlc-gen-rust",
            "wasm": {
                "url": "https://github.com/tunamaguro/sqlc-gen-rust/releases/download/v0.1.8/sqlc-gen-rust.wasm",
                "sha256": "44cc0547960768e5928a2dae8ffd467de5cc9121df0c70d307e66ddc0b3a5fa8"
            }
        }
    ],
    "sql": [
        {
            "schema": "./migrations/*.sql",
            "queries": "./queries/",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "src/",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "sqlx-postgres",
                        "overrides": [
                            {
                                "db_type": "uuid",
                                "rs_type": "sqlx::types::Uuid",
                                "copy_cheap": true
                            },
                            {
                                "db_type": "void",
                                "rs_type": "()",
                                "copy_cheap": true
                            }
                        ]
                    }
                }
            ]
        }
    ]
}