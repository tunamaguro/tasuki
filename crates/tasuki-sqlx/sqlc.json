{
    "version": "2",
    "plugins": [
        {
            "name": "sqlc-gen-rust",
            "wasm": {
                "url": "https://github.com/tunamaguro/sqlc-gen-rust/releases/download/v0.1.9/sqlc-gen-rust.wasm",
                "sha256": "ba3bfb9b5ce6c3121ada3afc3481d6adb8791afa72907d0026698db965b6dbef"
            }
        }
    ],
    "sql": [
        {
            "schema": "./migrations/*.sql",
            "queries": "./queries/",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "src/",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "sqlx-postgres",
                        "overrides": [
                            {
                                "db_type": "uuid",
                                "rs_type": "sqlx::types::Uuid",
                                "copy_cheap": true
                            },
                            {
                                "db_type": "void",
                                "rs_type": "()",
                                "copy_cheap": true
                            }
                        ]
                    }
                }
            ]
        }
    ]
}